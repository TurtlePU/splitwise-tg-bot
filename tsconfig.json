{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "outDir": "build",
    "strict": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "sourceMap": true,
    "baseUrl": ".",
    "paths": {
      "@locale": [ "src/locales/export" ],
      "@commands": [ "src/bot/commands/export" ],
      "@bot": [ "src/bot/main" ],
      "@storage": [ "src/storage/export.ts" ]
    }
  }
}
